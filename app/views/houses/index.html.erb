<h1>物件一覧</h1>
<table>
<thead>
<th>物件名</th>
<th>賃料</th>
<th>住所</th>
<th>築年数</th>
<th>備考</th>
</thead>
<tbody>
<% @house.each do |houses| %>
    <tr>
      <td><%= houses.property_name %></td>
      <td><%= houses.price %></td>
      <td><%= houses.street_address %></td>
      <td><%= houses.age_building %></td>
      <td><%= houses.remarks %></td>
    </tr>
    <% end %>   
  </tbody>
</table>
